Class {
	#name : #Punto,
	#superclass : #Object,
	#instVars : [
		'x',
		'y'
	],
	#category : #POO
}

{ #category : #accessing }
Punto >> x [

	^ x
]

{ #category : #accessing }
Punto >> x: anObject [

	x := anObject
]

{ #category : #accessing }
Punto >> y [

	^ y
]

{ #category : #accessing }
Punto >> y: anObject [

	y := anObject
]
